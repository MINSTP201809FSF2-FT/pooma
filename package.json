  },
  "homepage": "https://github.com/MINSTP201809FSF2-FT/pooma#readme",
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.11.0"

  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.15.4",
    "if-env": "^1.0.4"
  }
}
